<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register">

    <EditText
        android:id="@+id/companyName"
        android:layout_width="269dp"
        android:layout_height="63dp"
        android:layout_marginTop="64dp"
        android:ems="10"
        android:fontFamily="@font/bree_serif"
        android:hint="Nome da empresa"
        android:inputType="text"
        android:textColorHint="#8F8F8F"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/bree_serif"
        android:text="Registro"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="35dp"
        android:layout_height="31dp"
        android:onClick="returnScreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038"
        app:srcCompat="@drawable/resource_return" />

    <EditText
        android:id="@+id/companyDocument"
        android:layout_width="269dp"
        android:layout_height="63dp"
        android:ems="10"
        android:fontFamily="@font/bree_serif"
        android:hint="CNPJ da empresa
"
        android:inputType="number"
        android:textColorHint="#8F8F8F"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyName" />

    <EditText
        android:id="@+id/companyEmail"
        android:layout_width="269dp"
        android:layout_height="63dp"
        android:ems="10"
        android:fontFamily="@font/bree_serif"
        android:hint="E-mail"
        android:inputType="text"
        android:textColorHint="#8F8F8F"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyDocument" />

    <EditText
        android:id="@+id/companyPhone"
        android:layout_width="269dp"
        android:layout_height="63dp"
        android:ems="10"
        android:fontFamily="@font/bree_serif"
        android:hint="NÃºmero para contato"
        android:inputType="number"
        android:textColorHint="#8F8F8F"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyEmail" />

    <EditText
        android:id="@+id/companyPassword"
        android:layout_width="269dp"
        android:layout_height="63dp"
        android:ems="10"
        android:fontFamily="@font/bree_serif"
        android:hint="Senha"
        android:inputType="textPassword"
        android:textColorHint="#8F8F8F"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyPhone" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="296dp"
        android:layout_height="59dp"
        android:onClick="register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyPassword"
        app:layout_constraintVertical_bias="0.745"
        app:srcCompat="@drawable/btn_green_large" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bree_serif"
        android:text="Registrar"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintEnd_toEndOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="@+id/imageButton2"
        app:layout_constraintTop_toTopOf="@+id/imageButton2" />


</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:material="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.Register">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:fontFamily="@font/bree_serif"
        android:text="Registro"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="35dp"
        android:layout_height="31dp"
        android:onClick="returnScreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        app:srcCompat="@drawable/resource_return" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyNameLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView2"
        app:boxStrokeColor="@color/box_stroke_color"
        app:hintTextColor="@color/hint_color">


        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyName"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="Nome da empresa"
            android:inputType="text"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyDocumentLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/companyNameLayout"
        app:boxStrokeColor="@color/box_stroke_color"
        app:hintTextColor="@color/hint_color">



    <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyDocument"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="CNPJ da empresa"
            android:inputType="number"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyEmailLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/companyDocumentLayout"
        app:boxStrokeColor="@color/box_stroke_color"
        app:hintTextColor="@color/hint_color">


    <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyEmail"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="E-mail"
            android:inputType="text"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyPhoneLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/companyEmailLayout"
        app:boxStrokeColor="@color/box_stroke_color"
        app:hintTextColor="@color/hint_color">


    <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyPhone"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="NÃºmero para contato"
            android:inputType="number"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyPasswordLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/companyPhoneLayout"
        app:endIconMode="password_toggle"
        app:boxStrokeColor="@color/box_stroke_color">


    <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyPassword"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="Senha"
            android:inputType="textPassword"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/companyPasswordConfirmationLayout"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/companyPasswordLayout"
        app:endIconMode="password_toggle"
        app:boxStrokeColor="@color/box_stroke_color">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/companyPasswordConfirmation"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:fontFamily="@font/bree_serif"
            android:hint="Confirme sua senha"
            android:inputType="textPassword"
            android:textColorHint="#8F8F8F"
            android:textSize="22sp"
            android:textStyle="bold" />
    </com.google.android.material.textfield.TextInputLayout>

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="296dp"
        android:layout_height="59dp"
        android:onClick="register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/companyPasswordConfirmationLayout"
        app:layout_constraintVertical_bias="0.355"
        app:srcCompat="@drawable/btn_green_large" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bree_serif"
        android:text="Registrar"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintEnd_toEndOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="@+id/imageButton2"
        app:layout_constraintTop_toTopOf="@+id/imageButton2" />

</androidx.constraintlayout.widget.ConstraintLayout>
